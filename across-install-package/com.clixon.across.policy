    <description xml:lang="es">Ejecute across</description>
    <description xml:lang="fr">Exécutez across</description>
    <description xml:lang="it">Esegui across</description>
    <description xml:lang="ja">across 実行</description>
    <description xml:lang="ko">across 실행</description>
    <description xml:lang="nl">across uitvoeren</description>
    <description xml:lang="no">Kjør across</description>
    <description xml:lang="pt">Execute o across</description>
    <description xml:lang="ru">Запустите across</description>
    <description xml:lang="sv">Kör across</description>
    <description xml:lang="tr">across'yi çalıştır</description>
    <description xml:lang="zh_TW">運行 across</description>
