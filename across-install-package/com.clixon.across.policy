    <message xml:lang="zh_HK">需要驗證才能註冊 across 以自動啟動</message>
    <message xml:lang="zh_MO">需要驗證才能註冊 across 以自動啟動</message>
    <message xml:lang="zh_CN">需要验证才能注册 across 以自动启动</message>
    <message xml:lang="zh_SG">需要验证才能注册 across 以自动启动</message>
    <icon_name>across-Across</icon_name>
    <defaults>
      <allow_any>no</allow_any>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_self_keep</allow_active>
    </defaults>
    <annotate key="org.freedesktop.policykit.exec.path">/usr/local/bin/across/AcrossCenter</annotate>
  </action>

</policyconfig>
